  <script>
    loadProduct();
    async function loadProduct(){
        var resp = await fetch('/shop/product-list?pageIndex=1&pageSize=10');
        var productList = await resp.json();
        var productListOjb = document.getElementById("product-list");
        productListOjb.innerHTML = "";
        for(var i = 0; i< productList.length;i++){
            productListOjb.innerHTML += ` <div class="col">
            <div class="box_section">
              <div class="image_4"><img src="/static/images/${productList[i].Image}"></div>
              <h2 class="dolor_text">$<span style="color: #ebc30a;">${productList[i].Price}</span></h2>
              <h2 class="dolor_text">${productList[i].Name}</h2>
              <h2 class="dolor_text_1">${productList[i].Weight} kg</h2>
              <p class="tempor_text">${productList[i].Description} </p>
              <div class="buy_bt_1 active"><a href="#">Buy Now</a></div>
            </div>
          </div>`;
        }
      
    }
</script>
